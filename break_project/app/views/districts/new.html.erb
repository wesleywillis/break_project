<%= form_for @district, url: { action: @action}, method: @method do |f| %>

  <fieldset>
    <legend><%=@title%></legend>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <br>
    <br>
    <%= f.label :address%>
    <%= f.text_field :address %>
    <br>
    <br>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <br>
    <br>
    <%= f.label :county %>
    <%= f.text_field :county %>
    <br>
    <br>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <br>
    <br>
    <%= f.label :zip %>
    <%= f.text_field :zip %>
    <br>



  </fieldset>
  <%= f.submit %>
<% end %>
